{
  "hosting": {
    "public": "dist",
    "rewrites":
      [
        {
          "source": "/",
          "destination": "/home.html"
        },
        {
          "source": "/story/*",
          "destination": "/story.html"
        },
        {
          "source": "/about",
          "destination": "/about.html"
        }
      ]
    ,
    "headers": [ {
      "source" : "/service-worker.js",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "no-store"
      } ]
    } ]
  }
}
